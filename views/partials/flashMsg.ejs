<% if(danger && danger.length){ %>
    <p class="alert alert-danger   alert-alert-dismissible fade show position-fixed bottom-0 end-0 " role="alert">
        <button type="button" class="btn-close " data-bs-dismiss="alert" aria-label="Close"></button>
        <%= danger %>
    </p>
    <% } %>


        <% if(success && success.length){ %>
            <p class="alert alert-success alert-alert-dismissible fade show position-fixed bottom-0 end-0" role="alert">
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                <%= success %>
            </p>
            <% } %>


                <% if(passport && passport.length){ %>
                    <p class="alert alert-info alert-alert-dismissible fade position-fixed show bottom-0 end-0" role="alert">
                        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                        <%= passport %>
                    </p>
                    <% } %>



                        <style>
                            .alert {
                                z-index: 1100;
                            }
                        </style>